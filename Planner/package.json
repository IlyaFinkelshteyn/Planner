{
  "version": "1.0.0",
  "name": "planner",
  "private": true,
  "devDependencies": {
    "angular-mocks": "~1.6.2",
    "copyfiles": "~1.2.0",
    "jasmine-core": "~2.5.2",
    "karma": "~1.5.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-html-reporter": "~0.2.7",
    "karma-jasmine": "~1.1.0",
    "karma-junit-reporter": "~1.2.0",
    "karma-typescript": "~2.1.7",
    "rimraf": "~2.6.1"
  },
  "dependencies": {
    "@types/angular": "~1.6.7",
    "@types/angular-mocks": "^1.5.9",
    "@types/angular-route": "~1.3.3",
    "@types/angular-ui-bootstrap": "~0.13.40",
    "@types/jasmine": "^2.5.43",
    "angular": "~1.6.2",
    "angular-moment": "~1.0.1",
    "angular-route": "~1.6.2",
    "angular-ui-bootstrap": "~2.5.0",
    "angular-ui-event": "~1.0.0",
    "angular-utils-pagination": "~0.11.1",
    "bootstrap": "~3.3.7",
    "font-awesome": "~4.7.0",
    "jquery": "~3.1.1",
    "moment": "~2.17.1"
  },
  "scripts": {
    "postinstall:angular": "copyfiles -u 1 node_modules/angular/angular*.js* ./wwwroot/lib",
    "postinstall:angular-moment": "copyfiles -u 1 node_modules/angular-moment/angular*.js* ./wwwroot/lib",
    "postinstall:angular-route": "copyfiles -u 1 node_modules/angular-route/angular*.js* ./wwwroot/lib",
    "postinstall:angular-utils-pagination": "copyfiles -u 1 node_modules/angular-utils-pagination/dirPagination.js ./wwwroot/lib",
    "postinstall:angular-ui-bootstrap": "copyfiles -f node_modules/angular-ui-bootstrap/dist/ui-bootstrap* ./wwwroot/lib/angular-ui-bootstrap",
    "postinstall:angular-ui-event": "copyfiles -f node_modules/angular-ui-event/dist/event* ./wwwroot/lib/angular-ui-event",
    "postinstall:bootstrap": "copyfiles -u 3 node_modules/bootstrap/dist/**/bootstrap* node_modules/bootstrap/dist/**/glyphicons* ./wwwroot/lib/bootstrap",
    "postinstall:jquery": "copyfiles -u 3 node_modules/jquery/dist/jquery*.* ./wwwroot/lib/jquery",
    "postinstall:moment": "copyfiles -f node_modules/moment/moment.js node_modules/moment/min/moment.min.js ./wwwroot/lib/moment",
    "postinstall:font-awesome": "copyfiles -u 2 node_modules/font-awesome/css/* node_modules/font-awesome/fonts/* ./wwwroot/lib/font-awesome",
    "postinstall": "rimraf ./wwwroot/lib && npm run postinstall:angular && npm run postinstall:angular-moment && npm run postinstall:angular-route && npm run postinstall:bootstrap && npm run postinstall:jquery && npm run postinstall:moment && npm run postinstall:angular-utils-pagination && npm run postinstall:angular-ui-bootstrap && npm run postinstall:angular-ui-event && npm run postinstall:font-awesome"
  }
}